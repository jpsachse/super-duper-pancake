{
    "files.exclude": {
      "**/.git": true,
      "**/.DS_Store": true,
      "**/*.d.ts": { "when": "$(basename).ts" },
      "**/*.js": { "when": "$(basename).ts" },
      "**/*.js.map": true,
      "node_modules/**": true,
      ".nyc_output/**": true,
      "coverage/**": true
    },
    "editor.rulers": [100],
    "files.trimTrailingWhitespace": true
}